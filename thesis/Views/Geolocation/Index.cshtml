<link rel="stylesheet" href="~/css/geolocation.css" asp-append-version="true" />

<h1>Map location</h1>

<div class="form-group">
    <label for="exampleDropdown">Filter</label>
    <select class="form-control" id="exampleDropdown">
        <option value="">Dealers</option>
    </select>
</div>


<div id="map"></div>

<script>
    var map = L.map('map');
    map.setView([7.1907, 125.4553], 13);

    L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
        maxZoom: 19,
        attribution: '&copy; <a href="http://www.openstreetmap.org/copyright">OpenStreetMap</a>'
    }).addTo(map);



    var marker = L.marker([7.1907, 125.4553]).addTo(map);

</script>