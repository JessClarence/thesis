@model thesis.Models.Antemortem
<link href="~/css/inspectorSideNav.css" rel="stylesheet" />
@{
    ViewData["Title"] = "Create";
}
<section id="content">


    <!-- MAIN STARTS-->
    <main>
        <div class="FirstLayer">

            <div class="applications">
                <h1>Create</h1>

                <h4>Antemortem</h4>
                <hr />
                <div class="row">
                    <div class="col-md-4">
                        <form asp-action="Create">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="form-group">
                                <label asp-for="MeatInspectionReportId" class="control-label"></label>
                                <select asp-for="MeatInspectionReportId" class="form-control" asp-items="ViewBag.MeatInspectionReportId"></select>
                            </div>
                            <div class="form-group">
                                <input asp-controller="ConductOfInspections" asp-action="Create" type="submit" value="Create" class="btn btn-primary" />
                            </div>
                        </form>
                    </div>
                </div>

                <div>
                    <a asp-action="Index">Back to List</a>
                </div>

                @section Scripts {
                    @{
                        await Html.RenderPartialAsync("_ValidationScriptsPartial");
                    }
                }

            </div>

        </div>
    </main>
</section>
